{
    "name": "dzentota/identity",
    "description": "A secure and extensible authentication library for PHP applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Tatulchenkov",
            "email": "webtota@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/http-server-handler": "^1.0",
        "dzentota/session": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "nyholm/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Dzentota\\Identity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dzentota\\Identity\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}

